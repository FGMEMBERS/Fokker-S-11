<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>asi.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>glass.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation> 
    <type>material</type>
    <object-name>bezelinside</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission> 
      <red>   0.75 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>   0 </dep></entry>
      <entry><ind>  30 </ind><dep>  27 </dep></entry>
      <entry><ind>  50 </ind><dep>  74 </dep></entry>
      <entry><ind>  70 </ind><dep> 117 </dep></entry>
      <entry><ind>  90 </ind><dep> 161 </dep></entry>
      <entry><ind> 110 </ind><dep> 206 </dep></entry>
      <entry><ind> 130 </ind><dep> 250 </dep></entry>
      <entry><ind> 150 </ind><dep> 292 </dep></entry>
      <entry><ind> 170 </ind><dep> 314 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
